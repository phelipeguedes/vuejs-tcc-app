AA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,UAAA;AACA,OAJA;AAKA,KATA;AAWA,IAAA,IAXA,kBAWA;AACA,UAAA,GAAA,kDAAA;AACA,UAAA,MAAA,GAAA,KAAA,SAAA,CAAA,EAAA,GAAA,KAAA,GAAA,MAAA;AACA,UAAA,EAAA,GAAA,KAAA,SAAA,CAAA,EAAA,cAAA,KAAA,SAAA,CAAA,EAAA,IAAA,EAAA;AAEA,MAAA,KAAA,CAAA,MAAA,CAAA,WAAA,GAAA,cAAA,EAAA,GAAA,KAAA,SAAA,EAAA,IAAA,CAAA,YAAA;AACA,QAAA,KAAA,qBAAA,EAAA,yBAAA;AACA,QAAA,OAAA,CAAA,GAAA,WAAA,EAAA;AACA,OAHA,EAGA,KAHA,CAGA,KAAA,SAHA;AAIA;AApBA,GAlBA;AAyCA,EAAA,OAzCA,qBAyCA;AACA,SAAA,cAAA;AACA;AA3CA,CAAA","sourcesContent":["<template>\n    <div class=\"categorie-admin\">\n        <b-form>\n            <input type=\"hidden\" v-model=\"categorie.id\" id=\"categorie_id\">\n\n            <b-row>\n                <b-col md=\"12\" sm=\"12\">\n                    <b-form-group label=\"Nome\" label-for=\"categorie_name\">\n                        <b-form-input type=\"text\" v-model=\"categorie.name\" placeholder=\"Informe o nome da categoria\" required=\"\"/>\n                    </b-form-group>\n                </b-col>\n            </b-row>\n\n            <b-row>\n                <b-col md=\"12\" sm=\"12\">\n                    <b-form-group label=\"Categoria Principal\" label-for=\"categorie_pai\">\n                        <b-form-select v-model=\"categories.pai\" :options=\"options\" placeholder=\"Selecione a categoria pai\" required=\"\"/>\n                    </b-form-group>\n                </b-col>\n            </b-row>\n\n            <b-row>\n                <b-button variant=\"primary\" class=\"ml-3 mb-4\">Salvar</b-button>\n                <b-button variant=\"danger\" class=\"ml-3 mb-4\">Excluir</b-button>\n                <b-button class=\"ml-3 mb-4\">Cancelar</b-button>\n            </b-row>       \n        </b-form>\n\n        <b-table striped hover :items=\"categories\" :fields=\"fields\">\n            <template v-slot:cell(actions)=\"data\">\n                \n                <b-button class=\"mr-2\" variant=\"warning\" @click=\"loadCategorie(data.item)\">\n                    <i class=\"fa fa-pencil\"></i>\n                </b-button>                \n\n                <b-button class=\"mr-2\" variant=\"danger\" @click=\"remove(data.item)\">\n                    <i class=\"fa fa-trash\"></i>\n                </b-button>\n\n            </template>\n        </b-table>\n    </div>\n</template>\n\n<script>\n\nimport axios from 'axios'\n\nexport default {    \n    name: 'CategorieAdmin',\n    \n    data: function() {\n        return {\n            mode: 'save',\n            categorie: {},\n            categories: [],\n            fields: [\n                {key: 'id', sortable: true},\n                {key: 'postId', sortable: true},                \n                {key: 'name'},\n                {key: 'body'},\n                {key: 'actions', label: 'Actions'}\n            ]\n        }\n    },\n\n    methods: {\n        loadCategories() {\n            const url = `https://jsonplaceholder.typicode.com/comments`\n\n            axios.get(url)\n                .then(res => {\n                    this.categories = res.data\n                    console.log(this.categories)\n            });\n        },\n\n        save() {\n            const url = `https://jsonplaceholder.typicode.com/comments`\n            const method = this.categorie.id ? 'put' : 'post'\n            const id = this.categorie.id ? `/${this.categorie.id}` : ''\n            \n            axios[method](`${url}/${id}`, this.categorie).then(() => {\n                alert(`Categoria ${id} salva com sucesso!`)\n                console.log(`${id}`)\n            }).catch(this.showError)\n        }\n    }, \n\n    mounted() {\n        this.loadCategories()\n    },\n}\n</script>\n\n<style>\n    \n</style>"],"sourceRoot":"src/components/admin"}]}