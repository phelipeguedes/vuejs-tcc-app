nction() {\n        return {\n            stat: {}\n        }\n    },\n    \n    methods: {\n        getStats() {\n            axios.get(`${apiBaseURL}/stats`).then(res => {\n                this.stat = res.data\n            })\n        }\n    },\n    mounted() {\n        // ao carregar a página\n        this.getStats()\n    },\n}\n</script>\n\n<style>\n    .stats {\n        display: flex;\n        justify-content: space-between;\n        flex-wrap: wrap;\n    }\n</style>"]}]}