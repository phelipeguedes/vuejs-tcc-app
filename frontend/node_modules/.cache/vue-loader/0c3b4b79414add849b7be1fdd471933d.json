up>\n                </b-col>\n\n                <b-col md=\"6\" sm=\"12\" v-show=\"mode === 'save'\">\n                    <b-form-group label=\"Matrícula\" label-for=\"matricula\">\n                        <b-form-input id=\"matricula\" type=\"number\" v-model=\"aluno.matricula\" placeholder=\"Matrícula do aluno(a)\" required />\n                    </b-form-group>\n                </b-col>                           \n            </b-row>\n\n            <b-row>\n                <b-col md=\"6\" sm=\"12\" v-show=\"mode === 'save'\">\n                    <b-form-group label=\"Password\" label-for=\"password\">\n                        <b-form-input id=\"password\" type=\"password\" v-model=\"user.password\" placeholder=\"Digite uma senha\" required />\n                    </b-form-group>\n                </b-col>\n\n                <b-col md=\"6\" sm=\"12\">\n                    <b-form-group label=\"Email\" label-for=\"email\">\n                        <b-form-input :readonly=\"mode === 'remove'\" id=\"email\" type=\"email\" v-model=\"user.email\" placeholder=\"Informe o email\" required />                    \n                    </b-form-group>\n                </b-col> \n            </b-row>\n\n            <b-form-checkbox v-show=\"mode === 'save'\" id=\"user_admin\" v-model=\"user.admin\" class=\"mt-3 mb-3\">Administrador</b-form-checkbox>\n\n            <b-row>\n                <b-button variant=\"primary\" class=\"ml-3 mb-4\" v-if=\"mode === 'save'\" @click=\"save()\">Salvar</b-button>\n                <b-button variant=\"danger\" class=\"ml-3 mb-4\" v-if=\"mode === 'remove'\" @click=\"remove()\">Excluir</b-button>\n                <b-button class=\"ml-3 mb-4\" @click=\"reset()\">Cancelar</b-button>\n            </b-row>\n        </b-form>\n\n        <b-table striped hover :items=\"studentss\" :fields=\"fields\">\n            <template v-slot:cell(actions)=\"data\">\n                \n                <b-button class=\"mr-2\" variant=\"warning\" @click=\"loadStudents(data.item)\">\n                    <i class=\"fa fa-pencil\"></i> \n                </b-button>\n                \n                <b-button class=\"mr-2\" variant=\"danger\" @click=\"loadStudent(data.item, 'remove')\">\n                    <i class=\"fa fa-trash\"></i>\n                </b-button>\n\n            </template>\n        </b-table>\n    </div>\n</template>\n\n<script>\n\n//import apiBaseURL from \"@/global\";\nimport axios from 'axios';\n\nexport default {\n    name: 'userAdmin',\n    \n    data: function() {\n        return {\n            mode: 'save',\n            aluno: {},\n            alunos: [],\n            fields: [\n                {key: 'id', label: 'Id', sortable: true},\n                {key: 'name', label: 'Name', sortable: true},\n                {key: 'matricula', label: 'Matrícula'},\n                {key: 'curso', label: 'Curso'},\n                {key: 'email', label: 'Email'},\n                {key: 'actions', label: 'Ações'}\n            ]\n        }\n    },\n\n    methods: {\n\n        loadStudents() {  \n            const url = `http://localhost:3000/students`\n\n            axios.get(url)\n                .then(res => {\n                    console.log(this.students)\n                    this.students = res.data\n            });\n        },\n\n        save() {\n            const url = `http://localhost:3000`\n            const method = this.student.id ? 'put' : 'post'\n            const id = this.student.id ? `/${this.student.id}` : ''\n            \n            axios[method](`${url}/students/${id}`, this.student)\n                .then(() => {\n                    alert(`Usuário Salvo com Sucesso! ${id}`)\n                    this.loadstudents()\n                }).catch(this.showError)\n        },\n\n        reset() {\n            this.mode = 'save'\n            this.student = {}\n            this.loadstudents()\n        },\n\n        remove() {\n            const url = `http://localhost:3000`\n            const id = this.student.id\n            \n            axios.delete(`${url}/students/${id}`)\n                .then(() => {\n                    alert('Usuário removido com sucesso!')\n                    this.loadStudents()\n                }).catch(this.showError)\n        },\n\n        loadStudent(student, mode='save') {\n            this.mode = mode\n            // copiando os valores de this.student p/ student\n            this.student = { ...student }\n        },\n\n        showError() {\n            return alert('Erro!')\n        }\n\n    },\n    // ao carregar a página\n    mounted() {\n        this.loadStudents();\n    },\n}\n</script>\n\n<style>\n    \n</style>"]}]}